
<table class="table table-bordered" style="width:800px float:center">

  <tr>
    <th style="text-align:center">Vaccination</th>
    <th style="text-align:center">Clinic</th>
    <th style="text-align:center">Date administered</th>
    <th style="text-align:center">Expiry date</th>
    <th></th>
  </tr>

  <tr style="width: 100%" ng-repeat="history in histories">
    <td>{{history.vaccination.science_name}}</td>
    <td>{{history.clinic_name}}</td>
    <td>{{history.date_given}}</td>
    <td>{{history.expiry_date}}</td>
    <td><a href='/histories/{{history.id}}/edit'>Edit</a></td>
  </tr>


 </table>
 <a href='/histories/new' class="btn btn-primary">Add a vaccination</a>
